{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "no-console": "off",
    "eol-last": [2, "always"]
  },
  "globals": {
    "module": false,
    "require": false
  }
}